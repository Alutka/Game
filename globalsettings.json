{
  "globalConfig": {
    "storagePaths": {
      "static": "..\\Static",
      "devStatic": "..\\DevStatic",
      "definitions": "Definitions",
      "schemas": "Schemas",
      "maps": "Maps"
    },
    "files": {
      "mapExtension": ".gmap",
      "mapFile": "DefaultMap",
      "definitionsSchemaSuffix": "Schema",
      "biomesFile": "biomes",
      "sourcesFile": "sources",
      "resourcesFile": "resources",
      "rawsFile": "raws"
    }
  },
  "swaggerConfig": {
    "version": "v1",
    "title": "API",
    "endpointUrl": "/swagger/v1/swagger.json"
  },
  "ipRateLimiting": {
    "enableEndpointRateLimiting": false,
    "stackBlockedRequests": false,
    "realIpHeader": "X-Real-IP",
    "clientIdHeader": "X-ClientId",
    "httpStatusCode": 429,
    "generalRules": [
      {
        "endpoint": "*",
        "period": "1s",
        "limit": 100
      }
    ]
  }
}
