{
  "globalConfig": {
    "storagePaths": {
      "main": "..\\Static"
    },
    "mapExtension": ".map"
  },
  "swaggerConfig": {
    "version": "v1",
    "title": "API",
    "endpointUrl": "/swagger/v1/swagger.json"
  },
  "ipRateLimiting": {
    "enableEndpointRateLimiting": false,
    "stackBlockedRequests": false,
    "realIpHeader": "X-Real-IP",
    "clientIdHeader": "X-ClientId",
    "httpStatusCode": 429,
    "generalRules": [
      {
        "endpoint": "*",
        "period": "1s",
        "limit": 100
      }
    ]
  }
}
