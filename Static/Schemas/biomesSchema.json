{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$ref": "#/definitions/biomeSchema",
    "definitions": {
        "biomeSchema": {
            "type" : "object",
            "additionalProperties": false,
            "properties": {
                "$schema": {
					"title": "$schema"
				},
                "items": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/biome"
                    },
                    "minLength": 1
                }
            },
            "required": [
                "items"
            ]
        },
        "biome": {
            "type" : "object",
            "additionalProperties": false,
            "properties": {
                "name": {
                    "$ref": "enums.json#/definitions/biomeName"
                },
                "canTravel": {
                    "type": "boolean"
                }
            },
            "required": [
                "name",
                "canTravel"
            ]
        }
    }
}
