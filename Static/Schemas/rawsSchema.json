{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$ref": "#/definitions/rawSchema",
    "definitions": {
        "rawSchema": {
            "type" : "object",
            "additionalProperties": false,
            "properties": {
                "$schema": {
					"title": "$schema"
				},
                "items": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/raw"
                    },
                    "minLength": 1
                }
            },
            "required": [
                "items"
            ]
        },
        "raw": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "type": {
                    "$ref": "#/definitions/type"
                },
                "name": {
                    "$ref": "enums.json#/definitions/rawName"
                }
            },
            "required": [
                "name",
                "type"
            ]
        },
        "type": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "name": {
                    "$ref": "enums.json#/definitions/rawType"
                },
                "useRate": {
                    "type": "number",
                    "minimum": 1
                }
            },
            "required": [
                "name"
            ]
        }
    }
}
